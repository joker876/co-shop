{
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "ng",
          "commands": [
            "cd client"
          ]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "ng serve",
          "commands": ["cd client", "npm run start"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "server",
          "commands": ["cd server"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "nodemon",
          "commands": ["npm run server"]
        }
      ]
    }
  ],
  "files.associations": {
    ".xtm": "xml",
    ".ngI18nconfig": "json"
  }
}
